<!-- Example for openbox menu, YOU must add your other menu items! -->

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  
  <!-- The system applications menu generated by xdg-menu -->
  <menu id="xdg-menu" label="All applications" execute="cat /var/cache/xdg-menu/openbox/menu.xml"/>
  
  <separator />
  <menu id="system-menu"/>
  
  <!-- The resolution changer menu -->
  <menu id="resmenu" label="Resolutions" execute="/home/vnc-resmenu.sh"/>
  
  <separator />
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
